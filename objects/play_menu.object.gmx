{
    if(upKey)
    {
        if(index = 0) index = 8
        else index--
    }
    
    if(downKey)
    {
        if(index = 8) index = 0
        else index++
    }
    
    if(equipKey)
    {
        //inventory
        if(index = 0) instance_create(x,y,inventory_box)
        if(index = 1) instance_create(x,y,unequip_box)
        if(index = 2) player.draw_char_sheet = true
        if(index = 3) instance_create(x,y,spell_box)
        if(index = 4) instance_create(player.x,player.y,look_selector)
        if(index = 5)
        {
            if(global.item_index[# player.weapon, item_stat.wep_type] != weapon_type.gun) ds_list_insert(player.messages, 0, "You don't have a gun equipped!")
            else instance_create(player.x,player.y,shoot_selector)
        }
        if(index = 6) instance_create(x,y,topic_menu)
        if(index = 7) {}
        if(index = 8)
        {
            with(merchant) persistent = false
            with(player)
            {
                save_game()
                instance_destroy()
                with(console) instance_destroy()
                room_goto(start_room)
            }
        }
        instance_destroy()
    }
}
