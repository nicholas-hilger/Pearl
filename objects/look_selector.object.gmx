        found = true
        en = instance_place(x,y,enemy_parent)
        ds_list_insert(player.messages, 0, "You see a " + string(en.name) + ".")
        ds_list_insert(player.messages, 0, string(en.description))
    }
    if(place_meeting(x,y,item_parent))
    {
        found = true
        itm = instance_place(x,y,item_parent)
        ds_list_insert(player.messages, 0, "You see a " + string(itm.name) + " lying on the ground.")
    }
    if(place_meeting(x,y,gold))
    {
        found = true
        ds_list_insert(player.messages, 0, "You see some Zun on the ground.")
    }
    if(place_meeting(x,y,player))
    {
        found = true
        ds_list_insert(player.messages, 0, choose("You're so vain.", "Lookin' good!", "You twirl and check yourself out.", "Yep, you're still here.", "Is now really the time to navel gaze?", "You love your lips."))
    }
    if(place_meeting(x,y,merchant))
    {
        found = true
        merch = instance_place(x,y,merchant)
        ds_list_insert(player.messages, 0, "The " + string(merch.name) + ".")
        ds_list_insert(player.messages, 0, string(merch.description))
    }
    if(place_meeting(x,y,describable))
    {
        found = true
        des = instance_place(x,y,describable)
        ds_list_insert(player.messages, 0, string(des.description))   
