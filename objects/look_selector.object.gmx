        break_itm = instance_place(x,y,breakable)
        ds_list_insert(player.messages, 0, "You see a" + string(break_itm.name) + " here.")
    }
    if(!found) ds_list_insert(player.messages,0, "Nothing unusual here.")
    instance_destroy()
}

