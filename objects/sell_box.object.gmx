            <string>upKey = player.upKey
downKey = player.downKey
equipKey = keyboard_check_pressed(vk_enter) or gamepad_button_check_pressed(0, gp_face1)

current_item = ds_list_find_value(inventory, index)

if(ds_list_size(inventory) &gt; 0 and !console.vis)
{ 
    if(upKey)
    {
        if(index = 0) index = ds_list_size(inventory)-1
        else index--
    }
    
    if(downKey)
    {
        if(index = ds_list_size(inventory)-1) index = 0
        else index++
    }
    
    if(equipKey and current_item != item.none)
    {
        if(ds_list_size(player.inventory) &gt; 0)
        {
            ds_list_insert(player.messages, 0, "You sell the " + string(global.item_index[# current_item, item_stat.name]) + ".") 
            player.money += round(global.item_index[# current_item, item_stat.worth]*0.7)
            player.total_money += round(global.item_index[# current_item, item_stat.worth]*0.7)
            ds_list_delete(player.inventory, index)
            //ds_list_delete(inventory,index)
            if(index &gt; 0) index--
            if(ds_list_size(inventory) = 0) ds_list_add(inventory, item.none)
        }
        else ds_list_insert(player.messages, 0, "You can't sell that!")
    }
}
</string>
